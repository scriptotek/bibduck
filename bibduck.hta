<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
  <script type="text/javascript">
    window.moveTo(0,0);
    window.resizeTo(600, 250);
  </script>
  <HTA:APPLICATION id="bibduckHTA"
    applicationName="BIBDUCK"
    caption="yes"
    icon="images/Quack.ico"
    maximizeButton="yes"
    minimizeButton="yes"
    showInTaskbar="yes"
    windowState="normal"
    innerBorder="no"
    navigable="yes"
    scroll="auto"
    scrollFlat="yes"
    singleInstance="yes"
    sysMenu="yes"
    contextMenu="yes"
    selection="yes" 
    version="1.0" />
  <title>BIBDUCK</title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <link rel="stylesheet" type="text/css" href="style.css" />
  <!-- Libraries -->
  <script type="text/javascript" src="script/jquery-1.9.1.min.js"></script>
  <script type="text/javascript" src="script/jquery.hotkeys.js"></script>
  <script type="text/javascript" src="ecmaUtil/mswin/registry.js"></script>
  <!-- BIBDUCK script suite -->
  <script type="text/javascript" src="script/util.js"></script>
  <script type="text/javascript" src="script/bibsys.js"></script>
  <script type="text/javascript" src="script/bibduck.js"></script>
</head>
<body>

  <div id="settings-form" class="modal"> 
    <form class="inner">
      <table>
        <tr>
          <th valign="top">
            Mitt libnr.:
          </th>
          <td>
            <input type="text" /><br />
            <small>(skriv uten mellomrom, eks.: 1030310)</small>
          </td>
        </tr>
        <tr>
          <th valign="top">
            Standardprofil:
          </th>
          <td>
            <select id="active_profile"></select>
          </td>
        </tr>
        <tr>
          <th valign="top">
            Bakgrunnsinstans:
          </th>
          <td>
            <select id="auto_profile" disabled="disabled"></select>
          </td>
        </tr>
        <tr>
          <th valign="top">
            Stikkseddelskriver:
          </th>
          <td>
            <select id="stikk_skriver"></select><br />
            <small>(NB! Fungerer bare med kompatibelt stikkseddelscript)</small>
          </td>
        </tr>
      </table>
      <button>Avbryt</button>
      <button type="submit">Lagre</button>
    </form>
  </div>

  <div id="header">
    <div id="header-inner">
      <div id="rfidstatus">RFID: ?</div>
      <img src="loader.gif" id="loader-anim" />
      <button class="btn" id="clear-btn">Tøm logg</button>
      <button class="btn" id="settings-btn">Innstillinger</button>
      <button class="btn" id="kvakk-btn" title="Åpner kvakk.biblionaut.net i nettleseren din">Gi tilbakemelding</button>
      <!--
      <button class="btn" id="reload-btn">Reload plugins</button>
      -->
    </div>
  </div>
  <div id="right-pane">
    <h3>Vinduer:</h3>
    <div id="instances">
    </div>
    <div class="inner">
      <button class="new">Nytt vindu</button>
    </div>
  </div>
  <div id="log-outer">
    <div id="log"></div>
  </div>
  <div id="footer">
    <div id="statusbar">Statusbar</div>
  </div>

