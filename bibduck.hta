<!doctype html>
<html>
  <head>
  <title>BIBDUCK</title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <HTA:APPLICATION id="bibduckHTA"
    icon="images/Quack.ico"
    singleInstance="yes" />
  <script type="text/javascript">
    window.moveTo(0,0);
    window.resizeTo(700, 350);
  </script>
  <link rel="stylesheet" type="text/css" href="style.css" />
  <!-- Libraries -->
  <script type="text/javascript" src="script/jquery-1.10.2.min.js"></script>
  <script type="text/javascript" src="script/jquery.hotkeys.js"></script>
  <script type="text/javascript" src="ecmaUtil/mswin/registry.js"></script>
  <!-- BIBDUCK script suite -->
  <script type="text/javascript" src="script/util.js"></script>
  <script type="text/javascript" src="script/bibsys.js"></script>
  <script type="text/javascript" src="script/bibduck.js"></script>
</head>
<!--[if lt IE 7 ]> <body class="ie6"> <![endif]-->
<!--[if IE 7 ]>    <body class="ie7"> <![endif]-->
<!--[if IE 8 ]>    <body class="ie8"> <![endif]-->
<!--[if IE 9 ]>    <body class="ie9"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <body> <!--<![endif]-->
  <div id="settings-form" class="modal"> 
    <form class="inner">
      <table>
        <tr>
          <th valign="top">
            Mitt libnr.:
          </th>
          <td>
            <input type="text" /><br />
            <small>(skriv uten mellomrom, eks.: 1030310)</small>
          </td>
        </tr>
        <tr>
          <th valign="top">
            Standardprofil:
          </th>
          <td>
            <select id="active_profile"></select>
          </td>
        </tr>
        <tr>
          <th valign="top">
            Bakgrunnsinstans:
          </th>
          <td>
            <select id="auto_profile" disabled="disabled"></select>
          </td>
        </tr>
        <tr>
          <th valign="top">
            Stikkseddelskriver:
          </th>
          <td>
            <select id="stikk_skriver"></select><br />
            <small>(NB! Fungerer bare med kompatibelt stikkseddelscript)</small>
          </td>
        </tr
		<tr>
		  <td colspan="2">
		    Skriv ut automatisk
			<ul>
			<li>
			stikkseddel <input type="radio" name="autostikk_reg" id="autostikk_reg_alle" />
		      <label for="autostikk_reg_alle">etter alle utlån</label>
			<input type="radio" name="autostikk_reg" id="autostikk_reg_lib" />
		      <label for="autostikk_reg_lib">etter utlån til bibliotek</label>
			<input type="radio" name="autostikk_reg" id="autostikk_reg_ingen" />
		      <label for="autostikk_reg_ingen">aldri</label>
			</li>
			<li>
			<input type="checkbox" id="autostikk_res">
		      <label for="autostikk_res">hentehyllelapp etter hentebeskjed</label>
			</li>
			</ul>
          </td>
        </tr>
      </table>
      <button type="button">Avbryt</button>
      <button type="submit">Lagre</button>
    </form>
  </div>

  <div id="header">
    <div id="header-inner">
	  <div id="libnr"></div>
      <div id="rfidstatus">RFID: ?</div>
      <img src="loader.gif" id="loader-anim" />
      <button type="button" id="clear-btn">Tøm logg</button>
      <button type="button" id="settings-btn">Innstillinger</button>
      <button type="button" id="kvakk-btn" title="Åpner kvakk.biblionaut.net i nettleseren din">Tilbakemelding</button>
    </div>
  </div>
  <div id="right-pane">
    <h3>Vinduer:</h3>
    <div id="instances">
    </div>
    <div class="inner">
      <button class="new" type="button">Nytt vindu</button>
    </div>
  </div>
  <div id="log-outer">
    <div id="log">
		<audio src="assets/beep.mp3" id="ping"></audio>
		</div>
  </div>
  <div id="footer">
    <div id="status-right">?.? MB</div>
    <div id="statusbar">Statusbar</div>
  </div>

</body>
</html>

<!-- ----- ExeScript Options Begin -----
 ScriptType: window,invoker
 DestDirectory: temp
 Icon: default
 OutputFile: U:\umn-skript\bibduck\bibduck.exe
 32Bit: yes
 ----- ExeScript Options End ----- -->
